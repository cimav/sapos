#staff-schedule-header
  #staff-schedule-title
    %h2 Horario
  
  %center
    %table{:width=>"800px"}
      %tbody
        %tr
          %td{:align=>"right"}
            = hidden_field_tag :staff_id, staff.id
            = "Fecha de inicio:"
          %td
            %input{:type=>'text',:id=>'start_date',:readonly=>true}      
          %td{:align=>"right"}
            = "Fecha final:"
          %td
            %input{:type=>'text',:id=>'end_date'}      
          %td
            =link_to '  Buscar','#', :id=>'search-staff-schedule'

#staff-schedule-area

:javascript
  $( "#start_date" ).datepicker(config);
  $( "#end_date" ).datepicker(config);
  console.log("CONFIG DATES");
